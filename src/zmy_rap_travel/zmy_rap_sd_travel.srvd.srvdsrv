define service ZMY_RAP_SD_TRAVEL {
  expose ZMY_RAP_C_DEMO_01T as Travel;
  expose ZMY_RAP_C_DEMO_02  as Booking;
  expose /DMO/I_Agency      as Agency;
  expose /DMO/I_Customer    as Customer;
  expose /DMO/I_Flight      as Flight;
